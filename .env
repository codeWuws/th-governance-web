# Application Version Control
# Options: dev (development) | demo (demo version)
# dev: All modules support API calls
# demo: Only data governance module uses real API, others use mock data
VITE_APP_VERSION=dev

# Application Title
VITE_APP_TITLE=Medical Data Governance Platform

# API Base URL
VITE_APP_API_BASE_URL=/api

# API Request Timeout (milliseconds)
VITE_APP_API_TIMEOUT=10000

# Enable Dev Tools
VITE_APP_ENABLE_DEVTOOLS=false

# Enable Analytics
VITE_APP_ENABLE_ANALYTICS=false

# Log Level
VITE_APP_LOG_LEVEL=info

# Show Performance Monitor
VITE_APP_SHOW_PERFORMANCE=false
